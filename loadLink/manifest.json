{
  "manifest_version": 2,
  "name": "First Link",
  "version": "0.1",
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": ["jquery-3.3.1.min.js", "content.js"]
    }
  ],

  "browser_action": {
    "default_action": "icon.png"
  },

  "background": {
    "scripts": ["background.js"]
  }
}
