{
  "name": "Original Tab",
  "version": "0.1",
  "description": "Help remember your tab's origin",
  "manifest_version": 2,
  "permissions": ["storage", "tabs"],
  "browser_action": {},
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": ["jquery-3.3.1.min.js", "content.js"]
    }
  ],
  "background": {
    "scripts": ["background.js"]
  }
}
